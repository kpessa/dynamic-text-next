    {
      "KEYNAME": "TPNVolume",
      "DISPLAY": "Volume",
      "MNEMONIC": "Parenteral Nutrition Total Volume",
      "UOM_DISP": "mL/kg/day",
      "TYPE": "Diluent",
      "OSMO_RATIO": 0,
      "EDITMODE": "None",
      "PRECISION": 2,
      "SPECIAL": "",
      "NOTE": [
        {
          "TEXT": "(TPN + Lipid Volume)"
        },
        {
          "TEXT": "&nbsp;"
        },
        {
          "TEXT": "[f( var rv=''; var WT=parseFloat(me.getValue('DoseWeightKG'));"
        },
        {
          "TEXT": "if( WT === 0 ){ rv='Enter a weight'; }"
        },
        {
          "TEXT": "else if( WT<10 ){"
        },
        {
          "TEXT": "rv+='<u>Holliday-Segar Maintenance Fluid Calculation</u>\\n'"
        },
        {
          "TEXT": "rv+=' Less than 10 kg: 100 - 125 mL/kg/day'; }"
        },
        {
          "TEXT": "else if (WT>= 10) {"
        },
        {
          "TEXT": "rv+='<u>Holliday-Segar Maintenance Fluid Calculation</u>\\n'"
        },
        {
          "TEXT": "var BV = (WT<20) ? 1000 : 1500 ; /* base volume */"
        },
        {
          "TEXT": "var PKG = (WT<20) ? 50 : 20 ; /* per kilogram volume */"
        },
        {
          "TEXT": "  var WTRangeStart = (WT<20) ? 10 : 20 ; /* weight range start */"
        },
        {
          "TEXT": "var WTDIF = me.maxP(WT - WTRangeStart,2); /* kg over min for this weight range */"
        },
        {
          "TEXT": "var TV = me.maxP(BV + ( WTDIF * PKG ),2); /* total volume */"
        },
        {
          "TEXT": "var ENTRY = me.maxP(TV / WT,2); /* recommended entry */"
        },
        {
          "TEXT": "rv+=BV +' mL + ('+ WTDIF + ' kg*'+ PKG + ' mL/kg) = ' + TV"
        },
        {
          "TEXT": "+ ' mL/day&divide;' + WT +' kg = '+ ENTRY + ' mL/kg/day';"
        },
        {
          "TEXT": "}"
        },
        {
          "TEXT": "return rv; )]"
        }
      ],
