          "TEXT": ""
        },
        {
          "TEXT": "Initiate at 10%, advance by 2.5% to 5% to goal"
        },
        {
          "TEXT": "&nbsp;"
        },
        {
          "TEXT": "[f( var rv=\"\"; var DWKG=me.getValue('DoseWeightKG');"
        },
        {
          "TEXT": "var vol = ( me.getValue('admixture')==='2:1' ? me.getValue('NonLipidVolume') : me.getValue('TotalVolume') );"
        },
        {
          "TEXT": "rv = 'Dextrose 10% (est.) solution needs: ' + me.maxP( vol * 0.10 / DWKG, 2 ) + ' gm/kg/day \\n'"
        },
        {
          "TEXT": "rv += 'Dextrose 12.5% (est.) solution needs: ' + me.maxP( vol * 0.125 / DWKG, 2 ) + ' gm/kg/day \\n'"
        },
        {
          "TEXT": "rv += 'Dextrose 15% (est.) solution needs: ' + me.maxP( vol * 0.15 / DWKG, 2 ) + ' gm/kg/day \\n'"
        },
        {
          "TEXT": "rv += 'Dextrose 17.5% (est.) solution needs: ' + me.maxP( vol * 0.175 / DWKG, 2 ) + ' gm/kg/day \\n'"
        },
        {
          "TEXT": "rv += 'Dextrose 20% (est.) solution needs: ' + me.maxP( vol * 0.20 / DWKG, 2 ) + ' gm/kg/day \\n'"
        },
        {
          "TEXT": "return (rv||''); )]"
        }
